<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="false"
        android:weightSum="1" >

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton3"
            android:background="#00e53535"
            android:src="@drawable/menu_icon"
            android:contentDescription="@string/list_menu_icon"
            android:nestedScrollingEnabled="false"
            android:onClick="onClickMenu" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/menu_tolist"
            android:id="@+id/textView5"
            android:textSize="35sp"
            android:textIsSelectable="false"
            android:textAlignment="center"
            android:gravity="center_vertical"
            android:onClick="onClickTolist"
            android:nestedScrollingEnabled="false" />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="false"
        android:weightSum="1" >

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton7"
            android:background="#00e53535"
            android:src="@drawable/load_icon"
            android:contentDescription="@string/list_menu_icon"
            android:nestedScrollingEnabled="false"
            android:onClick="onClickLoad" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/menu_loadall"
            android:id="@+id/textView7"
            android:textSize="35sp"
            android:textIsSelectable="false"
            android:textAlignment="center"
            android:gravity="center_vertical"
            android:onClick="onClickLoad"
            android:nestedScrollingEnabled="false" />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="false"
        android:weightSum="1" >

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton9"
            android:background="#00e53535"
            android:src="@drawable/way_icon"
            android:contentDescription="@string/list_menu_icon"
            android:nestedScrollingEnabled="false"
            android:onClick="onClickPlace" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/menu_place"
            android:id="@+id/textView8"
            android:textSize="35sp"
            android:textIsSelectable="false"
            android:textAlignment="center"
            android:gravity="center_vertical"
            android:onClick="onClickPlace"
            android:nestedScrollingEnabled="false" />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="false"
        android:weightSum="1" >

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton11"
            android:background="#00e53535"
            android:src="@drawable/settings_icon"
            android:contentDescription="@string/list_menu_icon"
            android:nestedScrollingEnabled="false"
            android:onClick="onClickStyle" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/menu_theme"
            android:id="@+id/textView9"
            android:textSize="35sp"
            android:textIsSelectable="false"
            android:textAlignment="center"
            android:gravity="center_vertical"
            android:onClick="onClickStyle"
            android:nestedScrollingEnabled="false" />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="false"
        android:weightSum="1" >

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton13"
            android:background="#00e53535"
            android:src="@drawable/reload_icon"
            android:contentDescription="@string/list_menu_icon"
            android:nestedScrollingEnabled="false"
            android:onClick="onClickRefresh" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/menu_listreload"
            android:id="@+id/textView10"
            android:textSize="35sp"
            android:textIsSelectable="false"
            android:textAlignment="center"
            android:gravity="center_vertical"
            android:onClick="onClickRefresh"
            android:nestedScrollingEnabled="false" />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="false"
        android:weightSum="1" >

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton15"
            android:background="#00e53535"
            android:src="@drawable/info_icon"
            android:contentDescription="@string/list_menu_icon"
            android:nestedScrollingEnabled="false"
            android:onClick="onClickAbout" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/menu_about"
            android:id="@+id/textView11"
            android:textSize="35sp"
            android:textIsSelectable="false"
            android:textAlignment="center"
            android:gravity="center_vertical"
            android:onClick="onClickAbout"
            android:nestedScrollingEnabled="false" />

    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center_horizontal"
        android:gravity="bottom">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:weightSum="1" >

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton17"
                android:background="#00e53535"
                android:src="@drawable/delete_icon"
                android:contentDescription="@string/list_menu_icon"
                android:nestedScrollingEnabled="false"
                android:onClick="onClickDelete" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/menu_deleteall"
                android:id="@+id/textView12"
                android:textSize="35sp"
                android:textIsSelectable="false"
                android:textAlignment="center"
                android:gravity="center_vertical"
                android:onClick="onClickDelete"
                android:nestedScrollingEnabled="false" />

        </LinearLayout>
    </LinearLayout>
</LinearLayout>